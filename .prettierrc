{
  "plugins": [
    "prettier-plugin-go-template"
  ],
  "overrides": [
    {
      "files": ["*.html","*.xml"],
      "options": {
        "parser": "go-template",
        "goTemplateBracketSpacing": true,
        "bracketSameLine": true,
        "htmlWhitespaceSensitivity": "strict"
      }
    },
    {
      "files": ["*.js", "*.ts"],
      "options": {
        "useTabs": true,
        "printWidth": 120,
        "singleQuote": true
      }
    }
  ]
}